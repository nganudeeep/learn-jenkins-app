{
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "family": "JenkinsApp-Prod",
    "containerDefinitions": [
        {
            "name": "jenkinsapp",
            "image": "nginx:1.26-alpine",
            "portMappings": [
                {
                    "name": "nginx-80",
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true
        }
    ],
    "volumes": [],
    "networkMode": "awsvpc",
    "memory": "512",        
    "cpu": "256",
    "executionRoleArn": ""
}